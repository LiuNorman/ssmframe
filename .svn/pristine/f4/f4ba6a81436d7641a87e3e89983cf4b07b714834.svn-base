<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>JsonTest</title>
</head>
<body>
<div ng-app="myAppTest" ng-controller="customersCtrl"> 

<ul>
  <li ng-repeat="x in names">
    {{ x.Name + ', ' + x.Country }}
  </li>
</ul>

</div>
<script type="text/javascript" src="../../js/common/angular1.3.9/js/angular.min.js"></script>

<script>
var app = angular.module('myAppTest', []);
app.controller('customersCtrl', function($scope, $http) {
    $http.get("http://192.168.1.117:8098/cardcouponframe/data/json/custom.json").success(function(response) {
    		$scope.names = response.records;
    });
});
</script> 

</body>
</html>